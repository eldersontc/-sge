<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SGE.Entidades" namespace="SGE.Entidades.Inventarios">
  <class name="ProductoUnidad" schema="Inventarios">
    <id name="idProductoUnidad">
      <generator class="identity" />
    </id>
    <property name="idProducto" column="idProducto"/>
    <many-to-one name="unidad" column="idUnidad" class="Unidad" lazy="false"/>
    <property name="factor" column="factor"/>
  </class>
</hibernate-mapping>