<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SGE.Entidades" namespace="SGE.Entidades.Administracion">
  <class name="Numeracion" schema="Administracion">
    <id name="idNumeracion">
      <generator class="identity" />
    </id>
    <property name="descripcion" column="descripcion"/>
    <property name="documento" column="documento"/>
    <property name="automatico" column="automatico"/>
    <property name="serie" column="serie"/>
    <property name="numeroActual" column="numeroActual"/>
    <property name="longitudNumero" column="longitudNumero"/>
    <property name="impuesto" column="impuesto"/>
    <property name="porcentajeImpuesto" column="porcentajeImpuesto"/>
    <property name="activo" column="activo"/>
  </class>
</hibernate-mapping>