<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SGE.Entidades" namespace="SGE.Entidades.Ventas">
  <class name="FormaPago" schema="Ventas">
    <id name="idFormaPago">
      <generator class="identity" />
    </id>
    <property name="descripcion" column="descripcion"/>
    <property name="credito" column="credito"/>
    <property name="dias" column="dias"/>
    <property name="activo" column="activo"/>
  </class>
</hibernate-mapping>