<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="SGE.Entidades" namespace="SGE.Entidades.Finanzas">
  <class name="IngCaja" schema="Finanzas">
    <id name="idIngCaja">
      <generator class="identity" />
    </id>
    <many-to-one name="numeracion" column="idNumeracion" class="Numeracion" lazy="false"/>
    <property name="numero" column="numero"/>
    <property name="fechaCreacion" column="fechaCreacion"/>
    <many-to-one name="responsable" column="idResponsable" class="Empleado" lazy="false"/>
    <many-to-one name="moneda" column="idMoneda" class="Moneda" lazy="false"/>
    <many-to-one name="caja" column="idCaja" class="Caja" lazy="false"/>
    <property name="observacion" column="observacion"/>
    <property name="subTotal" column="subTotal"/>
    <property name="pcjImpuesto" column="pcjImpuesto"/>
    <property name="monImpuesto" column="monImpuesto"/>
    <property name="tipoCambio" column="tipoCambio"/>
    <property name="total" column="total"/>
    <property name="estado" column="estado"/>
  </class>
</hibernate-mapping>